[{
    "id": "deploy",
    "execute-command": "/root/deploy/deploy_front.sh",
    "response-message": "Executing deployment hook...",
    "trigger-rule": {
        "match": {
            "type": "value",
            "value": "true",
            "parameter": {
                "source": "payload",
                "name": "repository.is_private"
            }
        }
    },
    "pass-environment-to-command": [{
        "envname": "VERSION",
        "source": "payload",
        "name": "push_data.tag"
    }]
}]